@model Employee
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<h2 style="color:@ViewData["Color"]">@Model.Name</h2>
@Model.Salary


<hr />

<h3>@ViewData["Message"]</h3>
<h3>@ViewBag.Message</h3>
<hr />



<h3>@(int.Parse(ViewData["Temp"].ToString())+10)</h3>
<h3>@(ViewBag.Temp+10)</h3>
<hr />


<select class="form form-control">
    @foreach (var item in (List<string>)ViewData["Branches"])
    {
        <option>@item</option>
    }
</select>
<select class="form form-control">
    @foreach (var item in ViewBag.Branches)
    {
        <option>@item</option>
    }
</select>


