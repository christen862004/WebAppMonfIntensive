@model Department
@{
    ViewData["Title"] = "New";
}

<h1>New</h1>
<!--/Department/SaveNew?Name=SD&ManagerName=Ahmed-->
<form action="/Department/SaveNew" method="post">
    <div>
        <label>Name</label>
        <input name="Name" id="Name" value="@Model?.Name" class="form form-control" />
    </div>
    <div>
        <label>Manager Name</label>
        <input name="ManagerName" id="ManagerName" value="@Model?.ManagerName" class="form form-control" />
    </div>

    <input type="submit" value="Create" class="btn btn-success"/>

</form>
<a href="/Department/Index">Back To List</a>